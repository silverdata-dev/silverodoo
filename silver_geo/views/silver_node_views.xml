<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_silver_node_form_inherit_geo" model="ir.ui.view">
            <field name="name">silver.node.form.inherit.geo</field>
            <field name="model">silver.node</field>
            <field name="inherit_id" ref="silver_network.view_silver_node_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='network_graph']" position="before">
                    <page name="map_view" string="Mapa">
                        <field name="id" class="map-field" widget="node_map"/>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
