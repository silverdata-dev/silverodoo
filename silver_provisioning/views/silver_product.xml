<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_product_inherit_silver_pro"  model="ir.ui.view">
            <field name="name">product.product.form.inherit.silver_pro</field>
            <field name="model">product.product</field>

            <field name="inherit_id" ref="silver_product.product_product_inherit_silver"/>
            <field name="arch" type="xml">

 <xpath expr="//field[@name='service_type_id']" position="after">
                    <field name="linktype_id"/>
                </xpath>

            </field>

        </record>

        <record id="product_template_form_view_inherit_silver_pro" model="ir.ui.view">
            <field name="name">product.template.form.inherit.silver_pro</field>
            <field name="model">product.template</field>

            <field name="inherit_id" ref="silver_product.product_template_form_view_inherit_silver"/>
            <field name="arch" type="xml">

               <xpath expr="//field[@name='service_type_id']" position="after">
                    <field name="linktype_id"/>
                </xpath>


            </field>
        </record>
    </data>
</odoo>