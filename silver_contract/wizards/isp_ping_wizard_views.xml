<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="isp_ping_wizard_view_form" model="ir.ui.view">
            <field name="name">isp.ping.wizard.form</field>
            <field name="model">isp.ping.wizard</field>
            <field name="arch" type="xml">
                <form string="Resultado de Ping">
                    <group>
                        <field name="ping_output" nolabel="1"/>
                    </group>
                    <footer>
                        <button string="Cerrar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
